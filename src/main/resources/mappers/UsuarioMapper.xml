<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace = "edu.eci.cvds.persistence.myDAOImplement.mappers.UsuarioMapper">
    <insert parameterType = "map" id = "crearCategoria">
        INSERT INTO categoria
        (idCategoria, nombreCategoria,descripcion,fechaCreacion,fechaModif
        VALUES
        (#{idCategoria}, #{nombreCategoria}, #{descripcion},#{fechaCreacion}, #{fechaModif});
    </insert>




</mapper>
